               <FontAwesomeIcon fill="WHITE" glyphName="MINUS" size="1.2em" />
           </graphic>
       </Button>
      <TableView fx:id="checkoutViewer" layoutY="40.0" prefHeight="411.0" prefWidth="300.0">
